#允许rpc
enable-rpc=true
#允许所有来源, web界面跨域权限需要
rpc-allow-origin-all=true
#允许非外部访问
rpc-listen-all=true
#RPC端口, 仅当默认端口被占用时修改
#rpc-listen-port=6800
#token验证
rpc-secret=koolshare
#从会话文件中读取下载任务
input-file=/jffs/aria2/aria2.session
#在Aria2退出时保存`错误/未完成`的下载任务到会话文件
save-session=/jffs/aria2/aria2.session
#定时保存会话, 0为退出时才保存, 需1.16.1以上版本, 默认:0
save-session-interval=60
force-save=true


